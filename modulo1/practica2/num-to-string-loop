ORG 1000H
MSJ DB "INGRESE UN NUMERO: "
FIN DB ?
UNO DB "UNO"
XUNO DB ?
DOS DB "DOS"
XDOS DB ?
TRES DB "TRES"
XTRES DB ?
CUATRO DB "CUATRO"
XCUATRO DB ?
CINCO DB "CINCO"
XCINCO DB ?
SEIS DB "SEIS"
XSEIS DB ?
SIETE DB "SIETE"
XSIETE DB ?
OCHO DB "OCHO"
XOCHO DB ?
NUEVE DB "NUEVE"
XNUEVE DB ?

ORG 1500H
NUM DB ?

ORG 2000H
INITIALIZE: MOV DL, 2 ;SETEO BREAK CASE

LOOP: MOV BX, OFFSET MSJ
MOV AL, OFFSET FIN - OFFSET MSJ
INT 7
MOV BX, OFFSET NUM
INT 6 ;lectura del input en NUM

FIRST: CMP NUM, 49
JNZ SECOND
MOV BX, OFFSET UNO
MOV AL, OFFSET XUNO - OFFSET UNO
INT 7
JMP OK

SECOND: CMP NUM, 50
JNZ THIRD
MOV BX, OFFSET DOS
MOV AL, OFFSET XDOS - OFFSET DOS
INT 7
JMP OK

THIRD: CMP NUM, 51
JNZ FORTH
MOV BX, OFFSET TRES
MOV AL, OFFSET XTRES - OFFSET TRES
INT 7
JMP OK

FORTH: CMP NUM, 52
JNZ FIFTH
MOV BX, OFFSET CUATRO
MOV AL, OFFSET XCUATRO - OFFSET CUATRO
INT 7
JMP OK

FIFTH: CMP NUM, 53
JNZ SIXTH
MOV BX, OFFSET CINCO
MOV AL, OFFSET XCINCO - OFFSET CINCO
INT 7
JMP OK

SIXTH: CMP NUM, 54
JNZ SEVENTH
MOV BX, OFFSET SEIS
MOV AL, OFFSET XSEIS - OFFSET SEIS
INT 7
JMP OK

SEVENTH: CMP NUM, 55
JNZ EIGHTH
MOV BX, OFFSET SIETE
MOV AL, OFFSET XSIETE - OFFSET SIETE
INT 7
JMP OK

EIGHTH: CMP NUM, 56
JNZ NINTH
MOV BX, OFFSET OCHO
MOV AL, OFFSET XOCHO - OFFSET OCHO
INT 7
JMP OK

NINTH: CMP NUM, 57
JNZ OK
MOV BX, OFFSET NUEVE
MOV AL, OFFSET XNUEVE - OFFSET NUEVE
INT 7

OK: CMP NUM, 48
JNZ INITIALIZE
DEC DL
JNZ LOOP

INT 0
END
